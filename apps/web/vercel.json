{
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],

  "headers": [
      {
        "source": "/kuromoji/dict/(.+)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "localhost" },
          { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS" }
        ]
      }
    ]
  

}
